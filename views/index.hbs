                <div id = "list-friend" class="list-friend">
                    {{#each friend}}
                        <a  class="border-bt" name ="{{{this.fullname}}}" id ="{{{this.friendId}}}" onclick="checkUserClicked(this);">
                            <div id ="friend" class="friend">
                                <div class="new-message"></div>
                                <div class="friend-infor">
                                    <img class="img-list" src="https://i.pinimg.com/originals/ce/12/25/ce1225f92e766b3a87113dc69560e88f.jpg" alt="">
                                    <div class="name-friend">
                                        <div class="name-friend__infor">
                                            <span class="friend__fullname">{{this.fullname}}</span>
                                            <span data-id ="{{{this.friendId}}}" id = "friend__id-{{this.friendId}}" value = "{{this.friendId}}" class="friend__id">{{this.friendId}}</span>
                                        </div>
                                        <div class="name-friend__minute">
                                            <span {{#if this.rawDate}} class = "{{this.rawDate}}" {{/if}} {{#if this.rawDate}} id ="lasttimeReceiver-{{this.friendId}}" {{/if}} >{{#if this.time}} {{this.time}} {{/if}}</span>
                                        </div>
                                    </div>
                                </div>
                                <div class="content-message">
                                    <span style="font-weight:{{#if this.bold}} {{this.bold}} {{/if}};color:{{#if this.color}} {{this.color}} {{/if}}" name = "{{this.friendId}}" id="content-messag_detail-{{{this.friendId}}}" class="content-messag_detail">{{truncate this.text}}</span>
                                </div>
                            </div>
                        </a>
                    {{/each}}
                </div>
